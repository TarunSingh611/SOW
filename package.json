{
  "name": "sow-project",
  "version": "1.0.0",
  "description": "SOW Project - Full Stack Application",
  "scripts": {
    "prebuild": "cd client && npm install",
    "build": "cd client && npm run build",
    "prestart": "cd server && npm install",
    "start": "cd server && npm start",
    "dev": "concurrently \"cd client && npm run dev\" \"cd server && npm run dev\"",
    "install-all": "cd client && npm install && cd ../server && npm install"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "keywords": ["sow", "react", "fastify", "fullstack"],
  "author": "Tarun Singh Rajput",
  "license": "MIT"
}
